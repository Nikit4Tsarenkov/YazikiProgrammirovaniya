.stack 100h
.data
     mas db 0ffAh dup (' ')
  x dw  1
  y dw  5
  z dw  6 
  f dw ?        
          
  temp dd 12
  num dd 3698 
    
.code
  

       mov bx,9635	         ;F=12-(9635+X+Y)+Z+3698 = 7CF5  
		mov ax,x
		add bx,ax ;9635+x
		 
		 
		  add bx,y; 9635+x+y 
		  
		   sub temp,bx   ;12-(9635+X+Y)		
		   mov al,z
		   cbw
		    add temp,ax ; 12-(9635+X+Y)+Z
		     mov bx,temp   
		    add bx, num
		     					      
 
 mov ax,4c00h  
 int 21h

